{
  "name": "developer",
  "version": "1.0.0",
  "description": "",
  "main": "extension.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc --noEmit && node esbuild.config.js",
    "build:release": "BUILD_MODE=release tsc --noEmit && BUILD_MODE=release node esbuild.config.js",
    "watch": "tsc --watch --noEmit",
    "clean": "rm -f dist/*.js dist/*.js.map",
    "copy-files": "./scripts/copy-files.sh",
    "reload": "gdbus call --session --dest org.gnome.Shell --object-path /io/github/x7c1/Snappa --method io.github.x7c1.Snappa.Reload",
    "dev": "npm run build && npm run copy-files && npm run reload",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x7c1/snappa.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "bugs": {
    "url": "https://github.com/x7c1/snappa/issues"
  },
  "homepage": "https://github.com/x7c1/snappa#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@girs/adw-1": "1.6.0-4.0.0-beta.12",
    "@girs/clutter-14": "14.0.0-4.0.0-beta.12",
    "@girs/gdk-4.0": "4.0.0-4.0.0-beta.12",
    "@girs/gjs": "4.0.0-beta.12",
    "@girs/gnome-shell": "46.0.2",
    "@girs/gtk-4.0": "4.14.4-4.0.0-beta.12",
    "@girs/meta-14": "14.0.0-4.0.0-beta.12",
    "@girs/st-14": "14.0.0-4.0.0-beta.12",
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "overrides": {
    "@girs/cally-14": "14.0.0-4.0.0-beta.12",
    "@girs/clutter-14": "14.0.0-4.0.0-beta.12",
    "@girs/meta-14": "14.0.0-4.0.0-beta.12",
    "@girs/shell-14": "14.0.0-4.0.0-beta.12"
  }
}
